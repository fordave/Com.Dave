<Window x:Class="WpfWebDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfWebDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="1" Height="400">
        <StackPanel Margin="0" Orientation="Vertical" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
            <TextBox VerticalAlignment="Top" x:Name="navigateTxt" ></TextBox>
            <WebBrowser Margin="1"  x:Name="webBrowser1" Source="https://www.hao123.com" Height="300"/>
        </StackPanel>
    </Grid>
</Window>
